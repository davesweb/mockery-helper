{
    "name": "davesweb/mockery-helper",
    "description": "A set of helpers to create useful unit tests that use the Mockery library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Lemmens",
            "email": "DWLLemmens@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "mockery/mockery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpro/grumphp": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "Davesweb\\MockeryHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davesweb\\MockeryHelper\\Tests\\": "tests/"
        }
    }
}
